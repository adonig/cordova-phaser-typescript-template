{
    "name": "jp.codearts.cordova-phaser-typescript",
    "displayName": "Cordova Phaser TypeScript",
    "productName": "cordova-phaser-typescript",
    "version": "0.1.0",
    "description": "Phaser game template with cordova.",
    "main": "main.js",
    "scripts": {
        "test": "karma start",
        "webpack": "webpack --config webpack.dev.config.js --colors",
        "webpack:dist": "webpack --config webpack.dev.config.js --colors",
        "start": "webpack-dev-server --config webpack.dev.config.js --open",
        "start:dist": "webpack-dev-server --config webpack.dev.config.js --open",
        "assets": "node ./scripts/generateAssetsClass.js --dev",
        "assets:dist": "node ./scripts/generateAssetsClass.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fubira/cordova-phaser-typescript-template"
    },
    "author": "fubira <https://github.com/fubira>",
    "license": "MIT",
    "dependencies": {
        "@types/cordova": "0.0.34",
        "cordova-android": "^6.2.3",
        "cordova-ios": "^4.4.0",
        "cordova-plugin-admobpro": "^2.29.21",
        "cordova-plugin-console": "^1.0.7",
        "cordova-plugin-crosswalk-webview": "^2.3.0",
        "cordova-plugin-extension": "^1.5.2",
        "cordova-plugin-whitelist": "^1.3.2",
        "cordova-plugin-wkwebview-engine": "^1.1.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-console": {},
            "cordova-plugin-crosswalk-webview": {
                "XWALK_VERSION": "23+",
                "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
                "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-wkwebview-engine": {},
            "cordova-plugin-admobpro": {}
        },
        "platforms": [
            "android",
            "browser",
            "ios",
            "windows"
        ]
    },
    "devDependencies": {
        "@types/jasmine": "^2.5.54",
        "@types/node": "^8.0.27",
        "@types/power-assert": "^1.4.29",
        "clean-webpack-plugin": "^0.1.16",
        "copy-webpack-plugin": "^4.0.1",
        "dotenv-webpack": "^1.5.4",
        "expose-loader": "^0.7.3",
        "file-loader": "^0.11.2",
        "jasmine-core": "^2.8.0",
        "js-loader": "^0.1.1",
        "js-logger": "^1.3.0",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-webpack": "^2.0.4",
        "phantomjs-prebuilt": "^2.1.15",
        "phaser-ce": "^2.8.3",
        "power-assert": "^1.4.4",
        "shelljs": "^0.7.8",
        "stdout": "0.0.3",
        "ts-loader": "^2.3.5",
        "tslint": "^5.5.0",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.4.2",
        "webfontloader": "^1.6.28",
        "webpack": "^3.5.6",
        "webpack-dev-server": "^2.6.1",
        "webpack-shell-plugin": "^0.5.0",
        "xml2js": "^0.4.17"
    }
}
